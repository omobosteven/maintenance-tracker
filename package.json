{
  "name": "maintenance-tracker",
  "version": "1.0.0",
  "description": "Maintenance Tracker App is an application that provides users with the ability to reach out to operations or repairs department regarding repair or maintenance requests and monitor the status of their request.",
  "main": "./server/app.js",
  "scripts": {
    "start": "npm run build && node ./build/app.js",
    "pretest": "NODE_ENV=test npm run db-migrate",
    "test": "NODE_ENV=test mocha --require babel-core/register ./server/tests/index.js --exit --timeout 10000 --colors",
    "db-migrate": "babel-node ./server/models/users.js",
    "build": "babel server/ --out-dir build --copy-files --source-maps",
    "babel-node": "babel-node ./server/app.js",
    "start-dev": "nodemon --exec npm run babel-node",
    "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
    "heroku-postbuild": "npm run db-migrate"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "chai": "^4.1.2",
    "chai-http": "^4.0.0",
    "dotenv": "^5.0.1",
    "express": "^4.16.3",
    "is-number": "^6.0.0",
    "jsonwebtoken": "^8.2.1",
    "lodash": "^4.17.10",
    "mocha": "^5.1.1",
    "pg": "^7.4.3",
    "validator": "^10.2.0",
    "validatorjs": "^3.14.2"
  },
  "devDependencies": {
    "coveralls": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.11.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nodemon": "^1.17.4",
    "nyc": "^11.8.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/omobosteven/maintenance-tracker.git"
  },
  "author": "omobo stephen",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/omobosteven/maintenance-tracker/issues"
  },
  "homepage": "https://github.com/omobosteven/maintenance-tracker#readme"
}
